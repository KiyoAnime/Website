{
	"compilerOptions": {
		"strict": true,
		"noEmit": true,
		"jsx": "preserve",
		"target": "ESNext",
		"module": "ESNext",
		"isolatedModules": true,
		"esModuleInterop": true,
		"types": ["vite/client"],
		"moduleResolution": "node",
		"jsxImportSource": "solid-js",
		"paths": {
			"@/*": ["./src/*"]
		}
	},
	"exclude": ["node_modules", ".github", ".yarn"]
}
